<%- include('../header.ejs') %>
<section class="dashboard-wrap mtb-40">
    <div class="container">
        <div class="body-content">
            <div class="row">
                <div class="col-md-3">
                    <div class="dash-left">
                         
                    </div>   
                </div>
                <div class="col-md-9">
                    <div class="dash-right">
                        <div class="dash-header">
                            <div class="dash-title">
                              <h1 class="text-center text-danger"><span class="text-danger"></span>VIEW FORM</h1>
                            </div>
                            
                        </div>
                        <table class="table table-bordered table-striped">
                            <tr>
                                <th>S.No.</th>
                               
                                <th>Name</th>
                                <th>Email</th>
                                <th>Mobile</th>
                                <th>Address</th>
                                <th>College</th>
                                <th>Course</th>
                                <th>Stream</th>
                                
                            </tr>
                            <% var i=1 %>
                            
                         
                                <tr>
                                    <td><%= i++ %></td>
                                    
                                    <td><%= data.name %></td>
                                    <td><%= data.email %></td>
                                    <td><%= data.mobile %></td>
                                    <td><%= data.address %></td>
                                    <td><%= data.college %></td>
                                    <td><%= data.course %></td>
                                    <td><%= data.stream %></td>
                                </tr>
                               
                            
                              </table>
                         </div>   
                     </div>
                 </div>
             </div>
         </div>
       </section>
    
    <%- include('../footer.ejs') %>           
                             
                             
                      